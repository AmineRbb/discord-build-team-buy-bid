# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Discord Application ID (Client ID)
CLIENT_ID=your_application_client_id_here

# Discord Guild ID (Server ID) pour les tests (optionnel)
# Si défini, les commandes seront déployées uniquement sur ce serveur (plus rapide)
# Si non défini, les commandes seront déployées globalement (peut prendre 1h)
GUILD_ID=your_test_guild_id_here 